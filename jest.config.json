{
  "setupFiles": [
    "<rootDir>/jest-setup"
  ],
  "setupFilesAfterEnv": [
    "<rootDir>/jest"
  ],
  "snapshotResolver": "<rootDir>/jest-snapshot-resolver",
  "modulePathIgnorePatterns": [
    "<rootDir>/lib/"
  ],
  "transform": {
    "^.+\\.[jt]sx?$": "babel-jest",
    "\\.u32$": "<rootDir>/u32/jest-transform"
  }
}
