{
  "setupFiles": [
    "<rootDir>/jest-setup"
  ],
  "setupFilesAfterEnv": [
    "<rootDir>/jest"
  ],
  "snapshotResolver": "<rootDir>/jest-snapshot-resolver",
  "modulePathIgnorePatterns": [
    "<rootDir>/lib/"
  ],
  "transform": {
    "^.+\\.js$": "<rootDir>/prep/jest-transform",
    "^.+\\.ts$": "<rootDir>/as/jest-transform"
  }
}
