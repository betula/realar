{
  "name": "realar",
  "version": "0.0.2-beta.1",
  "description": "Next generation frontends",
  "repository": {
    "url": "https://github.com/betula/realar"
  },
  "bugs": {
    "url": "https://github.com/betula/realar/issues"
  },
  "license": "MIT",
  "scripts": {
    "bootstrap": "npx lerna bootstrap --no-ci",
    "clean": "npm run test:clean && npm run demo:clean && npm run build:clean && lerna clean --yes",
    "publish": "npm run test && npm run build && lerna publish",
    "start": "npm run bootstrap && npm run demo",
    "reinstall": "npm run clean && npm run bootstrap",
    "test": "cross-env REALAR_DEV=true jest --coverage",
    "test:clean": "jest --clearCache && rimraf coverage",
    "demo": "npm run demo:clean && cd demo && cross-env NODE_ENV=development parcel index.html --port 1210",
    "demo:clean": "cd demo && rimraf dist .cache",
    "build:clean": "rimraf build",
    "build": "rollup -c"
  },
  "author": "Slava Birch <mail@betula.co> (http://betula.co)",
  "keywords": [
    "state",
    "model",
    "reactive",
    "state management",
    "data flow",
    "react hooks",
    "micro frontends",
    "next"
  ],
  "main": "build/index.js",
  "module": "build/index.esm.js",
  "types": "realar.d.ts",
  "files": [
    "babel",
    "jest",
    "prod",
    "realar.d.ts"
  ],
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.3",
    "@babel/preset-env": "7.10.3",
    "@babel/preset-react": "7.10.1",
    "@rollup/plugin-node-resolve": "8.4.0",
    "babel-jest": "26.1.0",
    "cross-env": "7.0.2",
    "deasync": "0.1.20",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "jest": "26.1.0",
    "lerna": "3.22.1",
    "parcel": "1.12.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "rimraf": "3.0.2",
    "rollup": "2.21.0",
    "wabt": "1.0.17"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "alias": {
    "realar": "./build"
  }
}
