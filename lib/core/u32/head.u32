## define MEM_TAIL 0 # First free byte
## define SLICE 1
## define SEQ_ID 2
## define MEM 3

func init
  mem_tail_init() # MEM_TAIL
  mem_tail_inc(8) # First 8 values reserved by super global pointers

  slice_init()    # SLICE
  each_slice_init()


func each_slice_init
  seq_id_init()   # SEQ_ID
  mem_init()      # MEM

func start
  init()
  push()

func push
  slice_push()

func pop
  slice_pop()
